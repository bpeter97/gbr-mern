{
  "name": "gbr-management-system",
  "version": "1.0.0",
  "description": "A management system for Green Box Rentals.",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/**/*.test.js --timeout 5000",
    "test-watch": "nodemon --exec \"yarn run test\"",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \" npm run client\""
  },
  "author": "Brian Peter & Taylor Hartley",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "mongodb": "^3.0.7",
    "mongoose": "^5.0.17",
    "prop-types": "^15.6.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "expect": "^22.4.3",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "supertest": "^3.0.0"
  }
}
